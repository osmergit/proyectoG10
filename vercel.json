{
    "version": 2,
    "builds": [
        {
            "src": "./package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"            
        }
    ]
}